{
    "name": "liquidlab-agency/magento2-sameday-validation-fix",
    "description": "Magento 2 module that fixes the Sameday shipping method to properly validate Country field, which triggers shipping address validation and saving",
    "type": "magento2-module",
    "license": "MIT",
    "authors": [
        {
            "name": "Liquidlab",
            "email": "support@liquidlab.ro"
        }
    ],
    "keywords": [
        "magento2",
        "sameday",
        "shipping",
        "validation",
        "fix",
        "country-field"
    ],
    "require": {
        "php": "~7.1.0||~7.2.0||~7.3.0||~7.4.0||~8.1.0||~8.2.0||~8.3.0",
        "magento/framework": "^103.0",
        "magento/module-checkout": "^100.0",
        "sameday/magento-2-plugin": "^1.9"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Liquidlab\\SamedayValidationFix\\": ""
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
